{
  "name": "Kiosk-app",
  "displayName": "yKiosk",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "dev": "expo start -c",
    "dev:android": "NODE_ENV=development expo start --android --localhost",
    "dev:ios": "NODE_ENV=development expo start --ios",
    "real:android": "NODE_ENV=production expo start --android --localhost",
    "real:ios": "NODE_ENV=production expo start --ios",
    "sb:android": "STORYBOOK=true expo start --android --localhost",
    "sb:ios": "STORYBOOK=true expo start --ios",
    "web": "expo start --web",
    "test": "vitest run --reporter verbose",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "real_android": "bash ./scripts/build-android.sh",
    "real_IOS": "bash ./scripts/build-ios.sh"
  },
